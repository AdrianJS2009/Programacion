Proceso Ejercicio3
	
	Definir numRandom, numIntroducido Como Entero;
	numRandom<- 5;
	
	Escribir "Adivina el número, introduce un valor:" Sin Saltar;
	Leer numIntroducido;
	
	Si numRandom==numIntroducido Entonces
		Escribir "Has acertado el valor";
